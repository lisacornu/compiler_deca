class A {

    int x = 1;
    int y = 2;
    int z = 3;

    int initalpha(int a, int b) {
        return a+b;
    }

    int initbeta(int a, int b) {
        return a-b;
    }

    int superCalcul(int a, int b, int c, int d) {

        int alpha = initalpha(1,1); // = 2
        int beta = initbeta(2,1); // = 1
        a = a + x*y - beta; //1 + 2 - 1 = 2
        beta = 1;
        b = c = a + beta - alpha; //2 + 1 - 2 = 1
        return d - b; //4 - 1 = 3
    }

}

{
    A a = new A();
    int calcul;
    calcul = a.superCalcul(1,2,3,4);
    print(calcul);
}