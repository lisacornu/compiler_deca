{
    println("TEST AND");
    println("Devrait print : true");

    if (true && true) {
        println("true");
    }
    if (false && true) {
        println("false");
    }
    if (true && false) {
        println("false");
    }
    if (false && false) {
        println("false");
    }

    println("TEST OR");
    println("Devrait print : true true true");

    if (true || true) {
        println("true");
    }
    if (false || true) {
        println("true");
    }
    if (true || false) {
        println("true");
    }
    if (false || false) {
        println("false");
    }
}